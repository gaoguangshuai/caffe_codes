net: "saliency_hed_trainval.prototxt"
test_iter: 0
test_interval: 1000000
# lr for fine-tuning should be lower than when starting from scratch
#debug_info: true
base_lr: 5e-7
lr_policy: "step"
gamma: 0.1
iter_size: 10
# stepsize should also be lower, as we're closer to being done
stepsize: 10000
display: 300
max_iter: 100000
momentum: 0.9
weight_decay: 0.0002
snapshot: 5000
snapshot_prefix: "/home/zhangyu/data/vggnet/snap/hed/hed"
# uncomment the following to default to CPU mode solving
solver_mode: GPU 
device_id: 0

